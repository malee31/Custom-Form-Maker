<% switch(inputOptions.type) {
    case "password": %>
        <%- include("/partials/formComponents/passwordInput", {inputOptions}); %>
    <% break;
    case "color": %>
        <%- include("/partials/formComponents/colorInput", {inputOptions}); %>
    <% break;
    case "radio": %>
        <%- include("/partials/formComponents/radioInput", {inputOptions}); %>
    <% break;
    case "checkbox": %>
        <%- include("/partials/formComponents/checkboxInput", {inputOptions}); %>
    <% break;
    case "text":
    default: %>
        <%- include("/partials/formComponents/textInput", {inputOptions}); %>
<% } %>